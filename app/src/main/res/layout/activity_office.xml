<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.kb50group6.userapplication.OfficeActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30sp"
        android:text="@string/telephoneText"
        android:id="@+id/textNumber"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30sp"
        android:text="@string/addressText"
        android:id="@+id/textAddress"
        android:layout_below="@+id/textNumber"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30sp"
        android:text="New Text"
        android:id="@+id/textViewNumber"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/textNumber"
        android:onClick="onCallClick"
        android:clickable="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30sp"
        android:text="New Text"
        android:id="@+id/textViewAddress"
        android:layout_below="@+id/textNumber"
        android:layout_toEndOf="@+id/textAddress"
        android:onClick="onClickLocation" />

</RelativeLayout>
